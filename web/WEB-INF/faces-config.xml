<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>AdvReqBean</managed-bean-name>
        <managed-bean-class>ManageBeans.AdvReqBean</managed-bean-class>    
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>AddAdvReqBean</managed-bean-name>
        <managed-bean-class>ManageBeans.AddReqBean</managed-bean-class>
        
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>NewUserReqBean</managed-bean-name>
        <managed-bean-class>ManageBeans.NewUserReqBean</managed-bean-class>    
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>DetailReqBean</managed-bean-name>
        <managed-bean-class>ManageBeans.DetailReqBean</managed-bean-class>    
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>NotifReqBean</managed-bean-name>
        <managed-bean-class>ManageBeans.NotifReqBean</managed-bean-class>    
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>CampReqBean</managed-bean-name>
        <managed-bean-class>ManageBeans.CampReqBean</managed-bean-class>    
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>OrdReqBean</managed-bean-name>
        <managed-bean-class>ManageBeans.OrdReqBean</managed-bean-class>    
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>UsersReqBean</managed-bean-name>
        <managed-bean-class>ManageBeans.UsersReqBean</managed-bean-class>    
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>CustomersReqBean</managed-bean-name>
        <managed-bean-class>ManageBeans.CustomersReqBean</managed-bean-class>    
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>loginBean</managed-bean-name>
        <managed-bean-class>ManageBeans.LoginBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/registration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/error.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/account.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/advertisingDetails.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/error.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
